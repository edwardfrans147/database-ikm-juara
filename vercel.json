{
  "version": 2,
  "name": "database-ikm-juara",
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/shared/(.*)",
      "dest": "/shared/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/app.js"
    },
    {
      "src": "/",
      "dest": "/server/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/app.js"
    }
  ],
  "functions": {
    "server/app.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["sin1"],
  "github": {
    "silent": true
  }
}